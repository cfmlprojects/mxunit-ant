<B><FONT color=3D#a020f0>import</FONT></B> =
org.apache.commons.httpclient.HttpClient;
<B><FONT color=3D#a020f0>import</FONT></B> =
org.apache.commons.httpclient.MultiThreadedHttpConnectionManager;
<B><FONT color=3D#a020f0>import</FONT></B> =
org.apache.commons.httpclient.methods.GetMethod;

<I><FONT color=3D#b22222>/**
 * An example that performs GETs from multiple threads.
 *=20
 * @author Michael Becke
 */</FONT></I>
<B><FONT color=3D#a020f0>public</FONT></B> <B><FONT =
color=3D#a020f0>class</FONT></B> MultiThreadedExample {

    <I><FONT color=3D#b22222>/**
     * Constructor for MultiThreadedExample.
     */</FONT></I>
    <B><FONT color=3D#a020f0>public</FONT></B> MultiThreadedExample() {
        <B><FONT color=3D#a020f0>super</FONT></B>();
    }

    <B><FONT color=3D#a020f0>public</FONT></B> <B><FONT =
color=3D#a020f0>static</FONT></B> <B><FONT =
color=3D#a020f0>void</FONT></B> main(String[] args) {
       =20
        <I><FONT color=3D#b22222>// Create an HttpClient with the =
MultiThreadedHttpConnectionManager.
</FONT></I>        <I><FONT color=3D#b22222>// This connection manager =
must be used if more than one thread will
</FONT></I>        <I><FONT color=3D#b22222>// be using the HttpClient.
</FONT></I>        HttpClient httpClient =3D <B><FONT =
color=3D#a020f0>new</FONT></B> HttpClient(<B><FONT =
color=3D#a020f0>new</FONT></B> MultiThreadedHttpConnectionManager());
        <I><FONT color=3D#b22222>// Set the default host/protocol for =
the methods to connect to.
</FONT></I>        <I><FONT color=3D#b22222>// This value will only be =
used if the methods are not given an absolute URI
</FONT></I>        httpClient.getHostConfiguration().setHost(<B><FONT =
color=3D#bc8f8f>"jakarta.apache.org"</FONT></B>, 80, <B><FONT =
color=3D#bc8f8f>"http"</FONT></B>);
       =20
        <I><FONT color=3D#b22222>// create an array of URIs to perform =
GETs on
</FONT></I>        String[] urisToGet =3D {
            <B><FONT color=3D#bc8f8f>"/"</FONT></B>,
            <B><FONT color=3D#bc8f8f>"/commons/"</FONT></B>,
            <B><FONT color=3D#bc8f8f>"/commons/httpclient/"</FONT></B>,
            <B><FONT =
color=3D#bc8f8f>"http://svn.apache.org/viewvc/jakarta/httpcomponents/oac.=
hc3x/"</FONT></B>
        };
       =20
        <I><FONT color=3D#b22222>// create a thread for each URI
</FONT></I>        GetThread[] threads =3D <B><FONT =
color=3D#a020f0>new</FONT></B> GetThread[urisToGet.length];
        <B><FONT color=3D#a020f0>for</FONT></B> (<B><FONT =
color=3D#a020f0>int</FONT></B> i =3D 0; i &lt; threads.length; i++) {
            GetMethod get =3D <B><FONT color=3D#a020f0>new</FONT></B> =
GetMethod(urisToGet[i]);
            get.setFollowRedirects(<B><FONT =
color=3D#a020f0>true</FONT></B>);
            threads[i] =3D <B><FONT color=3D#a020f0>new</FONT></B> =
GetThread(httpClient, get, i + 1);
        }
       =20
        <I><FONT color=3D#b22222>// start the threads
</FONT></I>        <B><FONT color=3D#a020f0>for</FONT></B> (<B><FONT =
color=3D#a020f0>int</FONT></B> j =3D 0; j &lt; threads.length; j++) {
            threads[j].start();
        }
       =20
    }
   =20
    <I><FONT color=3D#b22222>/**
     * A thread that performs a GET.
	 */</FONT></I>
    <B><FONT color=3D#a020f0>static</FONT></B> <B><FONT =
color=3D#a020f0>class</FONT></B> GetThread <B><FONT =
color=3D#a020f0>extends</FONT></B> Thread {
       =20
        <B><FONT color=3D#a020f0>private</FONT></B> HttpClient =
httpClient;
        <B><FONT color=3D#a020f0>private</FONT></B> GetMethod method;
        <B><FONT color=3D#a020f0>private</FONT></B> <B><FONT =
color=3D#a020f0>int</FONT></B> id;
       =20
        <B><FONT color=3D#a020f0>public</FONT></B> GetThread(HttpClient =
httpClient, GetMethod method, <B><FONT color=3D#a020f0>int</FONT></B> =
id) {
            <B><FONT color=3D#a020f0>this</FONT></B>.httpClient =3D =
httpClient;
            <B><FONT color=3D#a020f0>this</FONT></B>.method =3D method;
            <B><FONT color=3D#a020f0>this</FONT></B>.id =3D id;
        }
       =20
        <I><FONT color=3D#b22222>/**
         * Executes the GetMethod and prints some satus information.
         */</FONT></I>
        <B><FONT color=3D#a020f0>public</FONT></B> <B><FONT =
color=3D#a020f0>void</FONT></B> run() {
           =20
            <B><FONT color=3D#a020f0>try</FONT></B> {
               =20
                System.out.println(id + <B><FONT color=3D#bc8f8f>" - =
about to get something from "</FONT></B> + method.getURI());
                <I><FONT color=3D#b22222>// execute the method
</FONT></I>                httpClient.executeMethod(method);
               =20
                System.out.println(id + <B><FONT color=3D#bc8f8f>" - get =
executed"</FONT></B>);
                <I><FONT color=3D#b22222>// get the response body as an =
array of bytes
</FONT></I>                <B><FONT color=3D#a020f0>byte</FONT></B>[] =
bytes =3D method.getResponseBody();
               =20
                System.out.println(id + <B><FONT color=3D#bc8f8f>" - =
"</FONT></B> + bytes.length + <B><FONT color=3D#bc8f8f>" bytes =
read"</FONT></B>);
               =20
            } <B><FONT color=3D#a020f0>catch</FONT></B> (Exception e) {
                System.out.println(id + <B><FONT color=3D#bc8f8f>" - =
error: "</FONT></B> + e);
            } <B><FONT color=3D#a020f0>finally</FONT></B> {
                <I><FONT color=3D#b22222>// always release the =
connection after we're done=20
</FONT></I>                method.releaseConnection();
                System.out.println(id + <B><FONT color=3D#bc8f8f>" - =
connection released"</FONT></B>);
            }
        }
      =20
    }
   =20
}
